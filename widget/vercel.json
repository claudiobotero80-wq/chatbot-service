{
  "buildCommand": "",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/widget.js",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=300" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/demo.html" }
  ]
}
